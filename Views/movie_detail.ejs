<div class="container-xxl py-5">
    <div class="row align-items-center position-relative mt-5 py-5">
        <div class="col-lg-3">
            <img src="<%= detail.poster %>" class="rounded" width="100%">
        </div>
        <div class="col-lg-6">
            <div class="content">
                <!-- <h5 style="font-size: 26px; margin-bottom: 10px; color: #ff4444    ; font-weight: 700;">New Movie</h5> -->
                <h1 class="text-white display-4" style="letter-spacing: 8px; font-weight: 800;"><%= detail.title %></h1>
                <div class="banner-meta py-3">
                    <ul class="d-flex flex-wrap">
                        <li class="quality" style="margin-right: 9px;">
                            <span>IMDB  <%=detail.imdb%>/10</span>
                        </li>
                        <li class="quality">
                            <span><%= detail.language %></span>
                            <span class="d-none"></span>
                        </li>
                    </ul>
                    <ul class="d-flex flex-wrap">
                        <li class="category">
                            <a href="#" style="text-transform: capitalize; letter-spacing: 2px;"><%= detail.genre %> </a>
                        </li>
                        <li class="release-time">
                            <span><i class="fa fa-calendar"></i> <%= detail.year %></span>
                        </li>
                    </ul>
                </div>
                <div class="movie-details-prime">
                    <ul>
                        <li class="share"><a href="#"><i class="icon-share-alt"></i> Share</a></li>
                        <li class="streaming">
                            <h6><%= detail.movie_streaming_chanel %></h6>
                            <span>Streaming Channels</span>
                        </li>
                        <li class="watch "><a href="/book_now/<%= detail.id %>" class="btnn popup-video text-light"><i class="fa fa-play"></i> Book Tickets</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="movie-details-btn">
            <a href="<%=detail.poster%>" class="download-btn">Download Now <img src="/img/download.svg" alt=""></a>
        </div>
    </div>
</div>
            </div>
        </div>