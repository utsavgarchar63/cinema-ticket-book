<link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
</head>
    
<style>
    body{
        font-family: poppins,sans-serif;
        background-color: black;
    }
    .banner-meta ul li {
        display: flex;
        align-items: center;
        margin-bottom: 15px;
        margin-right: 18px;
    }
    .banner-meta ul li.quality > span {
        font-size: 11px;
        text-transform: uppercase;
        color: #21232b;
        background: #fff;
        font-weight: 700;
        padding: 7px 11px;
        line-height: 1;
    }
    .banner-meta ul li.quality > span {
        font-size: 11px;
        text-transform: uppercase;
        color: #21232b;
        background: #fff;
        font-weight: 700;
        padding: 7px 11px;
        line-height: 1;
    }
    .banner-meta ul li.quality > span:last-child {
        background: transparent;
        color: #fff;
        margin-left: 9px;
        border: 2px solid #fff;
        padding: 5px 10px;
    }
    .banner-meta ul li.category > a {
        font-size: 14px;
        font-weight: 500;
        color: #e3dfdf;
    }
    .banner-meta ul li.release-time > span {
        font-size: 14px;
        font-weight: 500;
        color: #e3dfdf;
    }
    .banner-meta ul li.release-time > span > i {
        font-size: 12px;
        margin-right: 5px;
        color: #ff4444;
    }
    .banner-meta ul li.release-time > span + span {
        margin-left: 15px;
    }
    .banner-meta ul li.release-time > span {
        font-size: 14px;
        font-weight: 500;
        color: #e3dfdf;
    }
    p {
        font-size: 14px;
        font-weight: 500;
        line-height: 26px;
        color: #bcbcbc;
        margin-bottom: 15px;
    }
    .movie-details-prime ul {
        display: flex;
        align-items: center;
        max-width: 475px;
        background: #242c38;
        border-radius: 4px;
        border: 1px solid #343434;
        padding: 0px 25px 25px;
        margin-top: 35px;
        flex-wrap: wrap;
    }
    .movie-details-prime ul li.share {
        padding-right: 25px;
        margin-right: 25px;
        position: relative;
        text-align: center;
    }
    .movie-details-prime ul li.watch {
        margin-left: auto;
    }
    .movie-details-prime ul li {
        margin-top: 25px;
    }
    .movie-details-prime ul li.watch .btnn {
        font-size: 14px;
        padding: 13px 26px;
        letter-spacing: 0;
        background: transparent;
        border: 2px solid #ff4444   ;
        border-radius: 25px;
        transition: 0.4s all;
    }
    .movie-details-prime ul li.watch .btnn:hover {
        background: #ff4444 ;
        color: #000;
        border: 2px solid #ff4444   ;
    }
    li{
        list-style: none;
    }
    .btnn > i {
        margin-right: 7px;
    }
    .fa {
        font-weight: 900;
    }
    .movie-details-prime ul li.share::before {
        content: "";
        position: absolute;
        right: 0;
        top: 50%;
        transform: translateY(-50%);
        width: 1px;
        height: 56px;
        background: #fff;
        opacity: .15;
    }
    .movie-details-prime ul li.share a i {
        display: block;
        margin-bottom: 7px;
        font-size: 16px;
        color: #fff;
    }
    .movie-details-prime ul li.share a {
        display: block;
        color: #bcbcbc;
        font-size: 14px;
        font-weight: 500;
    }
    .movie-details-prime ul li.streaming h6 {
        font-size: 16px;
        margin-bottom: 5px;
        font-weight: 800;
        color: #fff;
    }
    .movie-details-prime ul li.streaming span {
        display: block;
        color: #bcbcbc;
        font-size: 14px;
    }
    .container-xxl {
        max-width: 1140px;
        margin: 0 auto;
    }
    .movie-details-btn .download-btn {
        background: #ff4444 ;
        text-transform: uppercase;
        font-size: 12px;
        font-weight: 500;
        color: #14141d;
        letter-spacing: 5px;
        position: absolute;
        bottom: 65px;
        right: -50px;
        transform: rotate(90deg);
        padding: 43px 49px;
        border-radius: 6px;
    }
    .download-btn img {
        max-width: 24px;
        transform: rotate(-90deg);
    }
</style>
</style>
